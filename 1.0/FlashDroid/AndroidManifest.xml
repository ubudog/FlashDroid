<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.ubudog.flashdroid"
      android:versionCode="1"
      android:versionName="1.0">


    <application android:icon="@drawable/flashlightsmall" android:label="@string/app_name">
        <activity android:name=".FlashDroid"
                  android:label="@string/app_name"
                  android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
    </application>
    
    <uses-permission android:name="android.permission.INTERNET"  />
    
<uses-sdk android:maxSdkVersion="12" android:targetSdkVersion="10" android:minSdkVersion="2"></uses-sdk>
       	
</manifest>